{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", "storybook-static/**"]
    },
    "lint": {},
    "format": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "dotEnv": [".env.development.local", ".env.local", ".env.development", ".env"]
    }
  },
  "globalEnv": ["NODE_ENV"]
}
